{
  "type": "module",
  "dependencies": {
    "@actual-app/api": "^24.12.0",
    "better-sqlite3": "^9.2.2",
    "concurrently": "8.2.0",
    "cors": "2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "ngrok": "4.3.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-select": "5.7.4",
    "xero-node": "4.36.0"
  },
  "scripts": {
    "dev": "concurrently --kill-others-on-fail --handle-input --prefix \"[{name}]\" --names \"server,client\" --prefix-colors \"bgBlue.bold,bgGreen.bold\" \"npm:dev:server\" \"npm:dev:client\"",
    "build": "vite build",
    "dev:client": "vite",
    "dev:server": "cross-env NODE_ENV=development node --no-deprecation src/index.js",
    "dev:bolt": "cross-env NODE_ENV=development BOLT_ENV=true node --no-deprecation src/index.js",
    "dev:local": "concurrently --kill-others-on-fail --handle-input --prefix \"[{name}]\" --names \"server,client\" --prefix-colors \"bgBlue.bold,bgGreen.bold\" \"npm run dev:bolt\" \"npm:dev:client\"",
    "dev:with-build": "npm run build && npm run dev:server",
    "serve": "vite preview",
    "start": "cross-env NODE_ENV=production node --no-deprecation src/index.js"
  },
  "devDependencies": {
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "@vitejs/plugin-react": "4.0.4",
    "autoprefixer": "10.4.15",
    "postcss": "8.4.28",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6",
    "vite": "4.4.9"
  }
}
