{
  "name": "xero-actual-sync",
  "version": "1.0.0",
  "description": "Sync between Xero and Actual Budget",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:client": "vite",
    "dev:server": "node --watch src/index.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build": "vite build",
    "serve": "vite preview",
    "start": "NODE_ENV=production node src/index.js"
  },
  "dependencies": {
    "@actual-app/api": "6.6.0",
    "concurrently": "8.2.0",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "ngrok": "4.3.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-select": "5.7.4",
    "xero-node": "4.36.0"
  },
  "devDependencies": {
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "@vitejs/plugin-react": "4.0.4",
    "autoprefixer": "10.4.15",
    "postcss": "8.4.28",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6",
    "vite": "4.4.9"
  }
}